{
  "name": "kriosmane/coinmarketcap-api",
  "description": "Laravel package for interacting with CoinMarketCap API",
  "keywords": [
    "laravel",
    "coin marketcap",
    "api",
    "pro"
  ],
  "type": "library",
  "license": "BSD-3-Clause",
  "homepage": "https://github.com/kriosmane/coinmarketcap-api",
  "authors": [
    {
      "name": "Krios Mane",
      "email": "krios.mane@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Kriosmane\\CoinMarketCap\\": "src/"
    }
  },
  "minimum-stability": "dev",
  "require": {
    "php": "^8.0|^8.1|^8.2|^8.3"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Kriosmane\\CoinMarketCap\\Providers\\CoinMarketCapServiceProvider"
      ],
      "aliases": {
        "CoinMarketCap": "Kriosmane\\CoinMarketCap\\Facades\\CoinMarketCap"
      }
    }
  },
  "prefer-stable": true
}
